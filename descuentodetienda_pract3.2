Proceso descuentodetienda
	definir resultado, cantidad Como Entero;
	definir mes Como Caracter;
	Escribir "introduce mes";
	Leer mes;
	
	escribir "ingrese cantidad total";
	Leer cantidad;
	
	si(mes="septiembre")Entonces
		resultado<-.85*cantidad;
		escribir "el total es: ", resultado;
	SiNo
		escribir "no hay descuento";
		escribir "el total es: ", cantidad;

		

	FinSi
	

		

	
FinProceso
