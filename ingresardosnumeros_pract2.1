Algoritmo dosnumeros
	definir numero1, numero2 Como Entero;
	//algoritmo que lee y compara dos numeros
	Escribir " introduce numero1 ";
	leer numero1;
	Escribir " introduce numero2 ";
	leer numero2;
	
	Si numero1 > numero2 Entonces
		escribir " numero1 mayor a numero2";
	sino 
		Si numero1=numero2 Entonces
			escribir  numero1, " igual a ", numero2;
		SiNo
			si numero2 > numero1 Entonces
				escribir numero2, " mayor a ", numero1;
			FinSi
		FinSi
	FinSi
	


	
	
	
	
	
	
	
FinAlgoritmo

